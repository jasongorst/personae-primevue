generator client {
  provider = "prisma-client"
  output   = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  id        Int    @id @default(autoincrement())
  player  String?
  mortalName  String?
  faeName  String?
  rank  String?
  kith  String?
  location  String?
  position  String?
  seeming  String?
  house  String?
  bannerhouse  String?
  description String?
  notes String?
  createdAt DateTime  @db.Timestamp(6) @default(now())
  updatedAt DateTime  @db.Timestamp(6) @updatedAt

  @@index([createdAt])
}
